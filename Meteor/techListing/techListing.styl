.techListing {
	width: 876rem;
	background: #fff;
	box-shadow: 0px 0px 15px 0px rgba(0, 0, 0, 0.05);
	padding: 30rem 47rem;
	margin-right: 13rem;
	}

	.techListing__header {
		display: flex;
		justify-content: space-between;
		align-items: center;
		padding-bottom: 30rem;
		margin-bottom: 30rem;
		border-bottom: solid 1px #e7e8e8;
		}

	.techListing__headline {
		font-family: Montserrat;
		font-weight: bold;
		color: #2d343a;
		font-size: 13rem;
		margin: 0;
		}